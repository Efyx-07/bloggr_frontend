{
    "extends": "./tsconfig.json",
    "compilerOptions": {
      "jsx": "react-jsx",
      "esModuleInterop": true,
      "isolatedModules": true,
      "module": "CommonJS",
      "target": "ES2015",
      "moduleResolution": "node"
    },
    "include": [
      "**/*.ts",
      "**/*.tsx"
    ],
    "exclude": [
      "node_modules"
    ]
  }